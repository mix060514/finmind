{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/.conda/bin/pipenv",
            "console": "integratedTerminal", 
            "args": [
                "run",
                "python",
                "-Xfrozen_modules=off",
                "${file}",
                "20241101", "20241103"
            ],
            "env": {
                "PYDEVD_DISABLE_FILE_VALIDATION": "1"
            }
        },
    ]
}


